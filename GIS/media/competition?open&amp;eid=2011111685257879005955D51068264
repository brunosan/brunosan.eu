myWindow.document.close();
myWindow.focus();
myWindow.print();
}
function showStatsHelp(){
  $('#statsHelp').fadeToggle('slow');
}
function loadUpOmniture(){
var sTitle = document.title;
var asTitleParts = sTitle.split("-");
while (sTitle.indexOf(",") > -1){
sTitle = sTitle.replace(",", "");
}
s_pageName = sTitle;
s_prop1 = sTitle;
s_hier1 += ", " + sTitle;
var s_account="wbnispwbiextcpe"; 
var script = document.createElement("script")
script.type = "text/javascript";
script.src = "https://siteresources.worldbank.org/scripts/omniture/g9/s_code_remote.js" ;
document.getElementsByTagName("body")[0].replaceChild(script, document.getElementById("remotejs"));
}
</script>
<script id="remotejs" type="text/javascript" src="https://siteresources.worldbank.org/scripts/omniture/g9/s_code_remote.js"></script>

