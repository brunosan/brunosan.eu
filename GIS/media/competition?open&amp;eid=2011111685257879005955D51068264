<script type="text/javascript">
  var authorPicts=[];
  var login_path='';
  $(function() {
    setupBannerExpandCollaps();
    //add the competition information
    addCompInfo();
    
  });           
            
  function addFieldData(fid,fval){
    if (fval.length==0){
      $(fid).parent().hide();
    }else{
      if ($.isArray(fval)){
         $(fid).html(fval.join(', '));
      }else{
         $(fid).html(fval);
      }
    }
  }
  
  function addTabData(tid,tval,tidx){
    if (tval.length==0){
      $('#info_tab').tabs( "remove" , tidx );
    }else{
      if ($.isArray(tval)){
         $(tid).html(tval.join(' '));
      }else{
         $(tid).html(tval);
      }
    }
  }
  
  function MakeEmailLinks(str,evn){
    var rtn=str;
    var emails=findEmailAddresses(str).split(',');
    for (var x=0;x<emails.length;x++){
      var stemp='<a href="mailto:eee?subject=World%20Bank%20Challenge&body=Regarding%20bbb">fff</a>';
      stemp=stemp.replace(/eee/g, emails[x]);
      stemp=stemp.replace(/fff/g, emails[x].replace('@','@ '));
      stemp=stemp.replace(/bbb/g, evn);
      rtn = rtn.replace(emails[x],stemp);
    }
    return rtn;
  }
  
  
  function addCompInfo(){
    var compevtID = getArgs("eid");
    var snodata='<div id="nodata" class="n02v9-recentlypub border-top1 padding-bottom12">';
    snodata+='<div class="n02v9-recentlypub-image"><a href="#nodata"><img class="defaultpic" width="82" height="82" src="/wbchallengefiles.nsf/files/nodata.png/$file/nodata.png" alt="" title="No challenge found"></a></div>';
    snodata+='<div class="n02v9-recentlypub-super-header-tag">&nbsp;</div>';
    snodata+='<div class="n02v9-recentlypub-headline">';
    snodata+='<p><a href="#nodata">A challenge reference was not specified or we did not find the specified challenge.  Please click on an appropriate challenge link.</a></p></div></div>';
    if (compevtID==''){
      //hide tabs and put up error message
      $('#comptitle').html('Unspecified Challenge Reference');
      $('#compintro').html(snodata);
      $('#tabs').hide();
      $('#rightinfo').hide();
      $('div.fauxborder.six').css('background','url("/wbchallengefiles.nsf/files/fauxborder2.gif/$file/fauxborder2.gif") repeat-y scroll 0 0 transparent');
      $('.lr02v1-subscribe').css('margin-bottom','50px');
      $('#loading').hide();
      $('.fauxborder').css('display','block');
     
    }else{
      // just in case there is an error
      $.ajaxSetup({"error":function(XMLHttpRequest,textStatus, errorThrown) {alert(errorThrown);}});
  
      //read the json feed
      var t=new Date();
      t=t.getTime();
      $.getJSON('/wbchallengecomp.nsf/compjson?openview&comp='+compevtID+'&cnt=300&t='+t, function(data){
        var thtml=''; var fcnt=0; var item;
        
        //Use this piece of code if you need to temporarily hard code an event end date
    	//$.each(data.competitions, function(i,item){
        //	if(item.eventid == "2011111685257879005955D51068264")
        //	{
        //    	item.edate = "2012-03-16";
        // 	}
        //});
        // find anything?
        if (data.numdocuments==0){
          //hide tabs and put up error message
          $('#comptitle').html('Unspecified Challenge Reference');
          $('#compintro').html(snodata);
          $('#tabs').hide();
          $('#rightinfo').hide();
          $('div.fauxborder.six').css('background','url("/wbchallengefiles.nsf/files/fauxborder2.gif/$file/fauxborder2.gif") repeat-y scroll 0 0 transparent');
          $('.lr02v1-subscribe').css('margin-bottom','50px');
          $('#loading').hide();
          $('.fauxborder').css('display','block');
          return;
        }
        
        
        // loop through each record
        for (var x=0;x<data.competitions.length;x++){
          if (data.competitions[x].eventid == compevtID){
           // goto real idea
            if (isLoggedIn()==true && data.competitions[x].status!='closed'){
              location.href = '/'+data.competitions[x].path;
              return;
            }
            // set global variable for path if they press login
            login_path= '/'+data.competitions[x].path;
 
            item=data.competitions[x];
            addFieldData('#comptitle',item.evname);
            document.title=(item.evname + ' | World Bank Institute');
        //    if (item.evpurpose <= 600){
              addFieldData('#compintro',item.evpurpose);
     //       }else{
  //            var stemp=item.evpurpose.substr(0,600)+'<div class="n02v9-recentlypub-readmore"><a href="#" onclick="showMoreRW(this);return false;">Read More »</a><span style="display:none">'+item.evpurpose.substr(600)+'</span></div>';
   //           addFieldData('#compintro',stemp);
  //          }
       //     var stemp=MakeEmailLinks(item.evcontact,item.evname);
            addFieldData('#comp_contact',item.evcontact);
            addFieldData('#comp_sponsor',item.m5);
            addFieldData('#comp_vpu', item.m6);            addFieldData('#comp_sector',item.m7);
            addFieldData('#comp_theme' ,item.m8);            addFieldData('#comp_topic',item.m9);
            addFieldData('#comp_region',item.m10);
            addFieldData('#comp_country',item.m11);
            addFieldData('#comp_mckinsey','');   // item.m12);
            
            d1=new Date();
            var ds=item.edate.replace(/-/g,',').split(',');
            var d2=new Date(ds[0],ds[1]-1,ds[2],23,59,59);            
            if (item.status=='open'){
              if (d1.getTime() <= d2.getTime()){                
                addFieldData('#comp_status','Open');
                $('#menu-new-idea-link').show();
                $('#menu-callout-list').show();
              }else if(item.m19.toString().indexOf('Votes On')!=-1){
                addFieldData('#comp_status','Open for Public Voting');              
              }else{
                addFieldData('#comp_status','Closed');
              }
            }else if(item.status=='closed'){
              addFieldData('#comp_status','Complete');
              login_path='';  //default back to compdb
            }else{
              addFieldData('#comp_status','Unknown');
            }
            
            addFieldData('#comp_begins', formatDateTime(item.sdate,false));
            addFieldData('#comp_ends', formatDateTime(item.edate,false));
            addFieldData('#comp_ideas', commaFormat(item.stats.ideas));
            addFieldData('#comp_comments', commaFormat(item.stats.coms));
            addFieldData('#comp_contributors', commaFormat(item.stats.conts));
            addFieldData('#comp_votes', commaFormat(item.stats.votes));
			//show the mass vote button if voting is on
			if(item.m19.toString().indexOf('Votes On')!=-1)
			{
				$('#menu-mass-vote-link').show();
                $('#menu-callout-list').show();
			}
			
            // link to new idea
            $('#newidea').attr('href','/'+item.path+'/newidea?open&login');
            
            // link to mass vote
            $('#massvote').attr('href','/'+item.path+'/massvote?open&login');
            // turn on the tab
            $('#info_tab').tabs({
              create: function(event, ui) {
                addTabData('#rules_tab',item.m13,0);
                addTabData('#legal_tab',item.m14,1);
                addTabData('#eligibility_tab',item.m15,2);
                addTabData('#participation_tab',item.m16,3);
                addTabData('#prize_tab',item.m18,4);
                addTabData('#evaluations_tab',item.m17,5);    
              },
              select: function(event, ui) {$(ui.tab).blur();}
            });
            // show stats?
            if (item.m19.toString().indexOf('Show Stats')>-1){
             if (item.m19.toString().indexOf('Comments On')==-1){$('#comp_comments').parent().hide();}
             if (item.m19.toString().indexOf('Votes On')==-1){$('#comp_votes').parent().hide();}
             $('#vitals').show();
            }
            break;
          }
        };
        // check to see if you must be logged in to see ideas
        if (item.m19.toString().indexOf('Show Ideas')>-1){          // now go create the tabs using same json data
          createIdeaTabs(compevtID, item.m19.toString());
        }else{
          $('#loading').hide();
          $('#ideas_tab_wrap').hide();
          $('.fauxborder').css('display','block');
        }
        // load omniture code
        loadUpOmniture();
      });
    }
  }
function showMoreRW(elm){
  $(elm).hide().parent().find('span').show();
}    
function buildIdeaHTML(compevtID,item,path,settings){
  
    var stemplate='<div class="n02v9-recentlypub border-top1 padding-bottom12 clearleft"><div style="float:left;">';
    stemplate+='<div class="n02v9-recentlypub-image"><img title="Login to see author image" class="defaultpic" width="60" height="70" src="/wbchallengefiles.nsf/files/defaultProfile.gif/$file/defaultProfile.gif" alt=""></div>';
    stemplate+='</div><div style="float:left;width:466px;padding-bottom:12px;"><div class="n02v9-recentlypub-super-header-tag">&nbsp;</div>';
    stemplate+='<div class="n02v9-recentlypub-headline">';
    stemplate+='<p><a href="aaa">ddd</a></p></div>';
    stemplate+='<div class="n02v9-recentlypub-author">Authored by eee&nbsp;|&nbsp;<span class="recentlypub">fff</span></div>';
    stemplate+='<div class="n02v9-recentlypub-text"><p>ggg</p></div>';
    stemplate+='<div class="n02v9-recentlypub-readmore" style="display:xxx;"><a href="aaa">Read More&nbsp;»</a></div>';
    stemplate+='<div class="n02v9-recentlypub-stars">';
    if (settings.indexOf('Comments On')>-1){
      stemplate+='<img width="13" height="12" src="/wbchallengefiles.nsf/files/comment_icon.png/$file/comment_icon.png" alt="" title=""><span>Comments&nbsp;(hhh)</span>';
    }
    //Don't want to display the vote average so commenting out the following lines
    //if (settings.indexOf('Votes On')>-1){
    //  stemplate+='<span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Votes&nbsp;(iii)&nbsp;&nbsp;&nbsp;</span>';
    //  stemplate+='<span id="voteStarDisplaySmall"><span class="all-stars" title="kkk Stars"><span class="some-stars" style="width:jjjpx;"></span></span></span>';
    //}
    stemplate+='</div></div></div>';
    var thtml=stemplate;
    if (isLoggedIn()==false || $('#comp_status').html()=='Complete'){
      if ($('#comp_status').html()=='Open'){
        thtml=thtml.replace(/aaa/g,('/wbchallengecomp.nsf/x/idea?open&eid='+compevtID+'&iid='+item.unid+'&es=2'));    //open  
      }else if ($('#comp_status').html()=='Closed'){
        thtml=thtml.replace(/aaa/g,('/wbchallengecomp.nsf/x/idea?open&eid='+compevtID+'&iid='+item.unid+'&es=1'));    //closed  
      }else{ // complete
        thtml=thtml.replace(/aaa/g,('/wbchallengecomp.nsf/x/idea?open&eid='+compevtID+'&iid='+item.unid+'&es=0'));    //complete   
      }
    }else{
      thtml=thtml.replace(/aaa/g,('/'+path+'/x/'+item.unid));    //url  
    }
    if ($.isArray(item.aq_1)){
      thtml=thtml.replace(/ccc/g,item.aq_1.join(', '));        //Sponsoring Unit
    }else{
      thtml=thtml.replace(/ccc/g,item.aq_1);        //Sponsoring Unit
    }
    thtml=thtml.replace(/ddd/g,item.ti);          //Title
    thtml=thtml.replace(/eee/g,item.au);          //Author
 
    thtml=thtml.replace(/fff/g,formatDateTime(item.dt,true));               //Date
    
    if (item.de.length >=250){                    //Description
      ds=item.de.substr(0,250)+' . . .';
      thtml=thtml.replace(/ggg/g,ds);             
      thtml=thtml.replace(/xxx/g,'block');             
    }else{
      thtml=thtml.replace(/ggg/g,item.de);        //Description
      thtml=thtml.replace(/xxx/g,'none');             
    }
    thtml=thtml.replace(/hhh/g,item.cc);          //Comment Count
    thtml=thtml.replace(/iii/g,item.vc);          //Vote Count
    thtml=thtml.replace(/jjj/g,(parseFloat(item.va)*13));          //Span width
    thtml=thtml.replace(/kkk/g,item.va);          //Vote Average
    
    if (isLoggedIn()==true){
      thtml=thtml.replace(/defaultpic/g,   item.ae.replace(/\x2E/g,'w1w').replace(/\x40/g,'z1z').replace(/-/g,'x1x')    );          //img class contains username
      thtml=thtml.replace('Login to see author image', 'No picture available for '+item.au);          //img class contains username
    }
    
    return thtml;
  }
  function returnPictureURL(afn){
   if (authorPicts[afn] == ''){
     $.getJSON('/wbchallengeprofiles.nsf/profilesjson?openagent&incnormal=1&filteron=email&filtertype=eq&filter='+escape(afn.replace(/w1w/g,'.').replace(/z1z/g,'@').replace(/x1x/g,'-')), function(data){
      // loop through each record
      for (var x=0;x<data.profiles.length;x++){
        if (data.profiles[x].picurl){
          var stemp=('img.'+afn);
          authorPicts[afn]=data.profiles[x].picurl;
          $(stemp).attr('src',authorPicts[afn]).attr('title','Picture of '+data.profiles[x].fn+' '+data.profiles[x].ln);
          break;
        }
      }
    });
   }
  }
  
  function createIdeaTabs(compevtID,settings){
    var snodata='<div id="nodata" class="n02v9-recentlypub border-top1 padding-bottom12">';
    snodata+='<div class="n02v9-recentlypub-image"><img class="defaultpic" width="82" height="82" src="/wbchallengefiles.nsf/files/nodata.png/$file/nodata.png" alt="" title="No data found"></div>';
    snodata+='<div class="n02v9-recentlypub-super-header-tag">&nbsp;</div>';
    snodata+='<div class="n02v9-recentlypub-headline">';
    snodata+='<p><a href="#nodata">No submissions have been found for this tab.  Please check back later.</a></p></div></div>';
    // hide the tabs
    if (settings.indexOf('Comments On')==-1){$('#mostdiscussed_label').hide();}
    if (settings.indexOf('Votes On')==-1){$('#toprated_label').hide();}
    
    //read the json feed
    var t=new Date();
    t=t.getTime();
    $.getJSON('/wbchallengecomp.nsf/ideasbycompjson?openview&cnt=300&comp='+compevtID+'&t='+t, function(data){
    	var thtml=''; var fcnt=0;
        if (data.numideas==0){
          data.ideas=[{unid:null}];
        }
        
        var latestArr=data.ideas.slice(0);
        latestArr.sort(function(a,b){
        
        // make a comment count field        
        a.cc=a.cms.length-1; //cntComments(a.cms);
        b.cc=b.cms.length-1;   //cntComments(b.cms);
        if (a.unid==null){a.dt="1901-01-01T00:00:00-00:00";}
        if (b.unid==null){b.dt="1901-01-01T00:00:00-00:00";}
        
        var ds=a.dt.substr(0,19).replace(/-/g,',').replace(/T/g,',').replace(/:/g,',').split(',');
        var d1=new Date(ds[0],ds[1]-1,ds[2],ds[3],ds[4],ds[5]);
        ds=b.dt.substr(0,19).replace(/-/g,',').replace(/T/g,',').replace(/:/g,',').split(',');
        var d2=new Date(ds[0],ds[1]-1,ds[2],ds[3],ds[4],ds[5]);
  
        //descending order          
        if (d1.getTime() > d2.getTime()){ return -1;}
        if (d1.getTime() < d2.getTime()){ return 1;}
        return 0;       
          
      	});
	  var randomArr=data.ideas.slice(0);
	  randomArr.sort(function(a,b){
		 var a1=Math.round(Math.random()*1);
		 var b1=Math.round(Math.random()*1);
		 if (b1==0){return (a1*-1);}
		 return a1;
	  });
	  var topRatedArr=data.ideas.slice(0);      
	  topRatedArr.sort(function(a,b){
		  if (a.unid==null){a.va=0;}
		  if (b.unid==null){b.va=0;}
		
		  //descending order          
		  if (parseFloat(a.va) > parseFloat(b.va)){ return -1;}
		  if (parseFloat(a.va) < parseFloat(b.va)){ return 1;}
		  return 0;       
	  });
	  var mostDiscArr=data.ideas.slice(0);      
	  mostDiscArr.sort(function(a,b){
		  if (a.unid==null){a.cc=0;}
		  if (b.unid==null){b.cc=0;}
		
		  //descending order          
		  if (parseInt(a.cc) > parseInt(b.cc)){ return -1;}
		  if (parseInt(a.cc) < parseInt(b.cc)){ return 1;}
		  return 0; 
	  });
	  
	  var allArr=latestArr.slice(0);
	  //Max increased so all submissions show
	  var max=1000;
	  
	  /* Latest tab not being used at present
	  var cnt=0;
	  for (var x=0;x<latestArr.length;x++){
		if (latestArr[x].unid != null && cnt < max){
		  $(buildIdeaHTML(data.comp.evid,latestArr[x],data.comp.path,settings)).appendTo('#latest_tab',data.comp.path);
		  cnt++;
		}
		if ( x==(latestArr.length-1) && $('#latest_tab').html().length==0 ){
		  $(snodata).appendTo('#latest_tab');
		}
	  }; latestArr='';
	  */
	  
	  //Shuffle tab
	  cnt=0;
	  for (var x=0;x<randomArr.length;x++){
		if (randomArr[x].unid != null && cnt < max){
		  $(buildIdeaHTML(data.comp.evid,randomArr[x],data.comp.path,settings)).appendTo('#shuffle_tab');
		  cnt++;
		}
		if ( x==(randomArr.length-1) && $('#shuffle_tab').html().length==0 ){
		  $(snodata).appendTo('#shuffle_tab');
		}
	  }; randomArr='';
	  
	  /* Top rated tab not being used at present
	  cnt=0;
	  for (var x=0;x<topRatedArr.length;x++){
		if (topRatedArr[x].unid != null && topRatedArr[x].va > 0 && cnt < max){
		  $(buildIdeaHTML(data.comp.evid,topRatedArr[x],data.comp.path,settings)).appendTo('#toprated_tab');
		  cnt++;
		}
		if ( x==(topRatedArr.length-1) && $('#toprated_tab').html().length==0 ){
		  $(snodata).appendTo('#toprated_tab');
		}
	  }; topRatedArr='';
	  */
	  
	  /* Most discussed tab not being used at present
	  cnt=0;
	  for (var x=0;x<mostDiscArr.length;x++){
		if (mostDiscArr[x].unid != null && cnt < max && mostDiscArr[x].cc >= 3){
		  $(buildIdeaHTML(data.comp.evid,mostDiscArr[x],data.comp.path,settings)).appendTo('#mostdiscussed_tab');
		  cnt++;
		}
		if ( x==(mostDiscArr.length-1) && $('#mostdiscussed_tab').html().length==0 ){
		  $(snodata).appendTo('#mostdiscussed_tab');
		}
	  }; mostDiscArr='';
	  */
	  
	  //All submissions tab
	  cnt=0;
	  for (var x=0;x<allArr.length;x++){
		if (allArr[x].unid != null){
		  $(buildIdeaHTML(data.comp.evid,allArr[x],data.comp.path,settings)).appendTo('#allsubmissions_tab');
		  cnt++;
		}
		if ( x==(allArr.length-1) && $('#allsubmissions_tab').html().length==0 ){
		  $(snodata).appendTo('#allsubmissions_tab');
		}
	  }; allArr='';
	  
	  $.getJSON('/wbchallengecomp.nsf/ideaswinbycompjson?openview&cnt=300&comp='+compevtID+'&t=' + t, function(data){      
      	var whtml = '';
    	var fhtml = '';
      
        if (data.numideas>0){
          data.ideas.sort(function(a,b){
		    return a.cdba - b.cdba;
		  });
        
          for (var i=0;i<data.ideas.length;i++){
            if (data.ideas[i].ca=='Winner' || data.ideas[i].ca=='Finalist'){
              if (data.ideas[i].ca=='Winner'){
              	whtml += buildIdeaHTML(data.comp.evid,data.ideas[i],data.comp.path,settings);
              }
              if (data.ideas[i].ca=='Finalist'){
                fhtml += buildIdeaHTML(data.comp.evid,data.ideas[i],data.comp.path,settings);
              }
            }
          }
    	}
    	
    	if (fhtml != '')
    	{
            $('<li id="finalist_label"><a href="#finalist_tab">Finalists</a></li>').prependTo('#ideas_tab ul');
            $('<div id="finalist_tab">' + fhtml + '</div>').insertBefore('#ideas_tab div:first');
        }
    	if (whtml != '')
    	{
			$('<li id="winner_label"><a href="#winner_tab">Winners</a></li>').prependTo('#ideas_tab ul');
            $('<div id="winner_tab">' + whtml + '</div>').insertBefore('#ideas_tab div:first');
        }
        
        $('#ideas_tab').tabs({select: function(event, ui) {$(ui.tab).blur();}});
	  	$('.ui-tabs-nav li').click(function(){$(this).find('a').click();});
	  	$('#loading').hide();
	  	$('.fauxborder').css('display','block');
	  
	  	if (isLoggedIn()==true){
			var pimgs=$('.n02v9-recentlypub-image img');
			for (var x=0;x<pimgs.length;x++){
				var stemp=$(pimgs[x]).attr('class');
				if (stemp.length>0){
				  if (typeof authorPicts[stemp] != 'undefined'){
					if (authorPicts[stemp].length > 0){
					  $(pimgs[x]).attr('src',authorPicts[stemp]);
					}
				  }else{
					authorPicts[stemp]='';
					returnPictureURL(stemp);
				  }
				}
			}       
	  	} 
      });
    });
  }
function commaFormat(s){
 return String(s).replace(/,/g,"").replace( /\d{1,3}(?=(\d{3})+(?!\d))/g , "$&,");
}
