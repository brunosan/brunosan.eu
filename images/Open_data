						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading"><span dir="auto">Open data</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
