<script>
function trim(str) {
   return str.replace(/^\s*|\s*$/g,"");
}

function CompruebaCampo (campo) {
	if (trim(campo.value).length == 0) {
		alert ('El campo "' + campo.label + '" no puede quedar en blanco');
		campo.focus();
		return false;
		}
	else
		return true;
}

function CompruebaMail(theElement)
{
var s = theElement.value;
//var filter=/^[A-Za-z][A-Za-z0-9_]*@[A-Za-z0-9_]+.[A-Za-z0-9_.]+[A-za-z]$/;
//var filter=/^[A-Za-z][A-Za-z0-9_.]*@[A-Za-z0-9_]+.[A-Za-z0-9_.]+[A-za-z]$/;
var filter=/^[A-Za-z][A-Za-z0-9_.]*@[A-Za-z0-9_]+.[A-Za-z0-9_.]+[A-za-z]$/;
 var filter=/^[_a-z0-9-]+(.[_a-z0-9-]+)*@[a-z0-9-]+(.[a-z0-9-]+)*(.[a-z]{2,3})$/
if (s.length == 0 ) return true;
   if (filter.test(s))
      return true;
   else {
     alert("La direccion de correo no parece valida");
     theElement.focus();
     return false;
     }
}


function EnviaForm (){
	if (CompruebaCampo (document.votar.email))
	if (CompruebaMail (document.votar.email))
	if (CompruebaCampo (document.votar.pass))
		document.votar.submit();
}
</script>

