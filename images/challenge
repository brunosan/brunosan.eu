<script type="text/javascript">
  var total_hero_items = 0;             // global for their scroller
  var corousal_time_interval = 0;       // global for their scroller
  var TabbedPanels1;                    // global for their tabs
  var globJson;                         // global to hold competition so we only have one call to db
  $(function() {
    // code to make the expand / collapse work in RED header bar
    setupBannerExpandCollaps()    
    // 1st thing set up the Hero Scroller, it calls subsequent code
    addHeroData();
    getNewsJson("wbchallengeblog1.nsf");
    getStoryJson("wbchallengeblog2.nsf");
  });           
            
            
  function addHeroData(){
    // template HTML need for each hero item in the scroller
    
    var stemplate='<li id="ooo" style="overflow: hidden; float: left; width: 785px; height: 264px;">';
    stemplate+='<div class="c15v3-rightcont">';
    stemplate+='<a href="aaa">';
    stemplate+='<img width="400" height="264" alt="" src="/wbchallengefiles.nsf/files/ggg/$file/ggg" />';
    stemplate+='</a></div><div class="c15v3-leftcont">';
    stemplate+='<div class="c15v3-superheadertag">bbb</div>';
    stemplate+='<div class="c15v3-title">';
    stemplate+='<a href="aaa">ccc</a>';
    stemplate+='</div><div class="c15v3-bodytext">';
    stemplate+='<p><span class="c15v3-date">eee&nbsp;thru&nbsp;fff&nbsp;&mdash;&nbsp;</span>ddd<span>&nbsp;';
    stemplate+='<a href="aaa">Read More �</a></span>';
    stemplate+='</p></div></div></li>';
    // just in case there is an error
 //   $.ajaxSetup({"error":function(XMLHttpRequest,textStatus, errorThrown) {alert(errorThrown);}});
    //read the json feed
    var t=new Date()
    t=t.getTime();
    $.getJSON('/wbchallengecomp.nsf/compsjson?openview&t='+t, function(data){
    
    	//Use this piece of code if you need to temporarily hard code an event end date
    	//$.each(data.competitions, function(i,item){
        //	if(item.eventid == "2011111685257879005955D51068264")
        // 	{
        //    	item.edate = "2012-03-16";
        //	}
        //});
    
      //Sort the JSON so the most recently opened competitions appear first	
      data.competitions.sort(function(a,b){
      	var firstDateNumber;
		var secondDateNumber;
				
		//Check we have a start date specified for the first value
		if(a.sdate == null){firstDateNumber = -1;}
		else
		{
			var dateOneParts;
			dateOneParts = a.sdate.split("-");
			firstDateNumber = new Date(dateOneParts[0], dateOneParts[1] - 1, dateOneParts[2]).getTime();
		}
		//Check we have a start date specified for the second value
		if(b.sdate == null){secondDateNumber = -1;}
		else
		{
			var dateTwoParts;
			dateTwoParts = b.sdate.split("-");
			secondDateNumber = new Date(dateTwoParts[0], dateTwoParts[1] - 1, dateTwoParts[2]).getTime();
		}
		//If the start dates are the same, sort alphabetucally
		if (firstDateNumber == secondDateNumber)
		{
			if(a.evname.toLowerCase() < b.evname.toLowerCase()){return -1;}
			else{return 1;}
		}
		//If first start date isn't specified, this sorts to the end
		else if (firstDateNumber == -1)
		{
				return 1;
		}
		//If second start date isn't specified, this sorts to the end
		else if (secondDateNumber == -1)
		{
				return -1;
		}
		//Otherwise compare as normal so newest dates appear first
		else
		{
				return secondDateNumber - firstDateNumber;
		}
      });
    
      var thtml=''; var fcnt=0; globJson=data;
      var d1, d2, ds;
      // loop through each record
      $.each(data.competitions, function(i,item){
        if (item.eventid != null){
          //ignore blanks and future events
          d1=new Date();
          ds=item.sdate.split('-');
          d2=new Date(ds[0],ds[1]-1,ds[2],0,0,0);
          if ((item.status!="closed") && (item.m1=="Yes") && (item.m19.toString().indexOf("Set Competition Live")>-1) && (d2 <= d1)){
  
            // take template and add data accordingly
            thtml=stemplate;
            fcnt++;
            thtml=thtml.replace('ooo',fcnt);
            if (isLoggedIn()==false || item.status == "closed"){
               thtml=thtml.replace(/aaa/g,('/wbchallengecomp.nsf/x/competition?open&eid='+item.eventid));    //url   
            }else{
               thtml=thtml.replace(/aaa/g,('/'+item.path));    //url  
            }
            thtml=thtml.replace(/bbb/g,''); // item.m6);        //VPU
            thtml=thtml.replace(/ccc/g,item.evname);
            thtml=thtml.replace(/ddd/g,item.m3);        //Teaser Text
            thtml=thtml.replace(/eee/g,formatDateTime(item.sdate,false));
            thtml=thtml.replace(/fff/g,formatDateTime(item.edate,false));
            thtml=thtml.replace(/ggg/g,item.m2);        //Hero Image
  
            //add to the scroller html
            $(thtml).appendTo('#featuredhero_ul');
          }
        }
      });
      
      // we got records to feature or not? if so, start the scroller, else hide it
      if (fcnt>0){
        total_hero_items=fcnt;
        $('#featuredhero_pg').html('1 / '+fcnt);
        corousal_time_interval=30000;
        carrousalStart();
      }else{
        $('#hero').hide();
      }
      // now go create the tabs using same json data
      createCompTabs();
    });    
  }
  
  function createCompTabs(){
    var stemplate='<div class="n02v9-recentlypub border-top1 padding-bottom12 clearleft">';
    stemplate+='<div class="n02v9-recentlypub-image"><a href="hhh"><img width="82" height="82" src="/wbchallengefiles.nsf/files/aaa/$file/aaa" alt="" title=""></a></div>';
    stemplate+='<div style="float:left;width:460px;">'; //<div class="n02v9-recentlypub-super-header-tag">bbb</div>';
    stemplate+='<div class="n02v9-recentlypub-headline">';
    stemplate+='<p><a href="hhh">ccc</a></p></div>';
//    stemplate+='<div class="n02v9-recentlypub-author">Sponsored by ddd<span class="recentlypub">eee thru fff</span></div>';
    stemplate+='<div class="n02v9-recentlypub-author"><span class="recentlypub">eee thru fff</span></div>';
    stemplate+='<div class="n02v9-recentlypub-text"><p>ggg</p></div>';
    stemplate+='<div class="n02v9-recentlypub-readmore"><a href="hhh">Read More �</a></div></div></div>';
    var snodata='<div id="nodata" class="n02v9-recentlypub border-top1 padding-bottom12">';
    snodata+='<div class="n02v9-recentlypub-image"><a href="#nodata"><img width="82" height="82" src="/wbchallengefiles.nsf/files/nodata.png/$file/nodata.png" alt="" title=""></a></div>';
    snodata+='<div class="n02v9-recentlypub-super-header-tag">&nbsp;</div>';
    snodata+='<div class="n02v9-recentlypub-headline">';
    snodata+='<p><a href="#nodata">No challenges have been found for this tab.  Please check back later.</a></p></div></div>';
    
    var thtml=''; var fcnt=0;
    var d1=new Date();var d2=new Date(); var ds='';
    $.each(globJson.competitions, function(i,item){
    
      //ignore blanks 
      if (item.eventid != null){
        d1=new Date();
        ds=item.sdate.split('-');
        d2=new Date(ds[0],ds[1]-1,ds[2]);
        
        //ignore future dates
        if ((item.m19.toString().indexOf("Set Competition Live")>-1) && (d2 <= d1)){
          thtml=stemplate;
          fcnt++;
          thtml=thtml.replace(/aaa/g,item.m4);        //Thumb nail image
          thtml=thtml.replace(/bbb/g,item.m6);        //VPU
          thtml=thtml.replace(/ccc/g,item.evname);
//          thtml=thtml.replace(/ddd/g,item.evsponsor);
          thtml=thtml.replace(/eee/g,formatDateTime(item.sdate,false));
          thtml=thtml.replace(/fff/g,formatDateTime(item.edate,false));
          thtml=thtml.replace(/ggg/g,item.m3);        //Teaser Text
          if (isLoggedIn()==false || item.status == "closed"){
             thtml=thtml.replace(/hhh/g,('/wbchallengecomp.nsf/x/competition?open&eid='+item.eventid));    //url   
          }else{
             thtml=thtml.replace(/hhh/g,('/'+item.path));    //url  
          }
  
          // do closing soon
          d3=new Date();
          d1=new Date();
          d1.setHours(23,59,59,0);
          d1.setDate(d1.getDate()+7);
          ds=item.edate.split('-');
          d2=new Date(ds[0],ds[1]-1,ds[2],23,59,59);
          if (d2.getTime() < d1.getTime() && d2.getTime() > d3.getTime() && item.status != "closed"){
            $(thtml).appendTo('#tab1');
          }
  
          // do closed
          d1=new Date();
          ds=item.edate.split('-');
          d2=new Date(ds[0],ds[1]-1,ds[2],23,59,59);
          if (d2.getTime() <= d1.getTime() && item.status != "closed"){
            $(thtml).appendTo('#tab5');
          }
  
          // do popularity
          if (d2.getTime() > d1.getTime() && parseInt(item.stats.conts) >= 5 && item.status != "closed"){
            $(thtml).appendTo('#tab3');
          }
          // do open
          if (d2.getTime() > d1.getTime() && item.status != "closed"){
            $(thtml).appendTo('#tab4');
          }
          // do recently posted
          d1=new Date();
          d1.setHours(0,0,0,0);
          d1.setDate(d1.getDate()-7);
          ds=item.sdate.split('-');
          d2=new Date(ds[0],ds[1]-1,ds[2]);
          if (d2.getTime() >= d1.getTime() && item.status != "closed"){
            $(thtml).appendTo('#tab2');
          }
            
          // do complete
          if (item.status == "closed"){
            $(thtml).appendTo('#tab6');
          }
        }
      }
      if (i==(globJson.competitions.length-1)){
        // select a non empty tab
        TabbedPanels1 = new Spry.Widget.TabbedPanels("TabbedPanels1");    
        var bSel = false;
        var stemp=$('#tab1').html();
        if (stemp!='' && bSel==false){
          TabbedPanels1.showPanel(0);
          bSel=true;
        }
        stemp=$('#tab2').html();
        if (stemp!='' && bSel==false){
          TabbedPanels1.showPanel(1);
          bSel=true;
        }
        stemp=$('#tab3').html();
        if (stemp!='' && bSel==false){
          TabbedPanels1.showPanel(2);
          bSel=true;
        }
        stemp=$('#tab4').html();
        if (stemp!='' && bSel==false){
          TabbedPanels1.showPanel(3);
          bSel=true;
        }
        stemp=$('#tab5').html();
        if (stemp!='' && bSel==false){
          TabbedPanels1.showPanel(4);
          bSel=true;
        }
        stemp=$('#tab6').html();
        if (stemp!='' && bSel==false){
          TabbedPanels1.showPanel(5);
          bSel=true;
        }        
        if (bSel==false){
          TabbedPanels1.showPanel(0);
        }
        
        // put in no data stuff if necessary
        var stemp=$('#tab1').html();
        if (stemp==''){$(snodata).appendTo('#tab1');};
        stemp=$('#tab2').html();
        if (stemp==''){$(snodata).appendTo('#tab2');};
        stemp=$('#tab3').html();
        if (stemp==''){$(snodata).appendTo('#tab3');};
        stemp=$('#tab4').html();
        if (stemp==''){$(snodata).appendTo('#tab4');};
        stemp=$('#tab5').html();
        if (stemp==''){$(snodata).appendTo('#tab5');};
        stemp=$('#tab6').html();
        if (stemp==''){$(snodata).appendTo('#tab6');};
      }
      
    });
    
  }
  
  /*   This is Phile Blog Stuff  */
function getFirstSentence(text){
	var result = text.substring(0, text.indexOf(".")+1);
	return result;
  }
function checkIsImage(imgFiles){
        var defurl = "/wbchallengefiles.nsf/files/newspaper.png/$file/newspaper.png";
        var allowedTypes = [".png",".jpg",".jpeg",".gif",".bmp"];
        for (var x=0;x<imgFiles.length;x++){
          var imgType = imgFiles[x].substr( imgFiles[x].length - 4, imgFiles[x].length );
	  if($.inArray(imgType, allowedTypes) !== -1  && imgFiles[x].toLowerCase().indexOf('thumb') !== -1) {
            defurl=imgFiles[x];
            break;
          }
        }
	return defurl;
  }
function isValidDate(dt) {
	var valid = "false";
      var ds=dt.substr(0,20).replace(/T/g,' ');
      var dt=new Date(ds);   
	var today = new Date();
	if(today.getTime() >= dt.getTime()) {
		valid = "true";
	}
	return valid;
}
function isFeatured(categories){
	var featured = false;
	if($.inArray("Featured", categories)>-1) {
		featured = true;
	}
	return featured;
}
function getPostType(categories){
	var type = "";
	if($.inArray("News", categories)>-1) {
		type = "News";
	} else if($.inArray("Announcement", categories)>-1) {
		type = "Announcement";
	}
	return type;
}
function formatDate(d){
	var date = new Date(d),
	day = date.getDate(),
	year = date.getFullYear(),
	mon = ["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],
	month = mon[date.getMonth()];
	return month+" "+day+", "+year;
}
function getNewsHtml(jsonObj, type) {
	var html = "";
	html+="<div class=\"tit1-cont\">";
	html+="<p>";
	html+="<a href=\""+jsonObj.url+"\">"+jsonObj.summary+"</a>";
	html+="</p>";
	html+="<div class=\"titledate\">"+formatDate(jsonObj.date)+" | "+type+"</div>";
	html+="</div>";
	return html;
}
function getStoryHtml(jsonObj) {
	var html = '';
	html+='<div class="n06v1-clusters-cont1"><a href="'+jsonObj.url+'"><div class="storyImgDiv" style="background-image:url('+checkIsImage(jsonObj.files)+'); background-repeat: no-repeat; background-position: left top;"></div></a>';
	html+='<div class="n06v1-superheader-title"></div>';
	html+='<div class="n06v1-headline">';
	html+='<a href="'+jsonObj.url+'">'+jsonObj.summary+'</a>';
	html+='</div>';
	html+='<div class="n06v1-bodytext">'+getFirstSentence(jsonObj.body)+'</div>';
	html+='<div class="n06v1-clusters-actionlink">';
	html+='<a href="'+jsonObj.url+'"> Read More �</a>';
	html+='</div>';
	html+='</div>';
	return html;
}
function getPaddingHtml(storyCount) {
	var html = "",
	columns = 3 - storyCount,
	i = 0;
	for (i=0; i<=columns; i++){
		html+="<div class=\"n06v1-clusters-cont1\" style=\"width: 167px;\">&nbsp;</div>";
	}
	return html;
}
function getStoryJson(instance) {
	$.getJSON('/'+instance+'/latestEntriesJSON?openview', function(data){
		var storyHtml = "<h1>Stories</h1>",
		MAX_STORIES = 3,
		results = data.blogentries,
		num = results.length-2,
		storyCount = 0,
		i = 0,
		post = "";
		
		for(i=0;i<=num;i++){
			if(isValidDate(results[i].time) === "true"){
				if(isFeatured(results[i].categories)){
					if (storyCount < MAX_STORIES) {
						storyHtml+=getStoryHtml(results[i]);
						storyCount++;
					}
				}
			}
		}
		if(storyCount<1){
			$('#stories').hide();
		} else {
			storyHtml+=getPaddingHtml(storyCount);
			$('#stories').html(storyHtml);
		}
	});
}
  
function getNewsJson(instance) {
	$.getJSON('/'+instance+'/latestEntriesJSON?openview', function(data){
		var newsHtml = "<p class=\"title\">WHAT'S NEW</p>",
		MAX_NEWS = 5,		results = data.blogentries,
		num = results.length-2,
		newsCount = 0,
		i = 0,
		post = "";
		
		for(i=0;i<=num;i++){
			if(isValidDate(results[i].time) === "true"){
				if(isFeatured(results[i].categories)){
					post = getPostType(results[i].categories);
					if (post ==="News" || post ==="Announcement"){
						if (newsCount < MAX_NEWS) {
							newsHtml+=getNewsHtml(results[i], post);
							newsCount++;
						}
					}
				}
			}
		}
			
		if(newsCount<1){
			$('#newsparent').hide();
		} else {
			newsHtml+="<div class=\"n02v10-Itemlistlink padding-bottom13\"><a href=\"/"+instance+"\">View All What's New �</a></div></div>";
			$('#news').html(newsHtml);
		}		
	});	
}
</script>

