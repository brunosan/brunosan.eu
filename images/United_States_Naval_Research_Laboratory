						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading"><span dir="auto">United States Naval Research Laboratory</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
