<script type='text/javascript'>
var tilejson = {
    scheme: 'tms',
    tiles: [
        'http://a.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.png',
        'http://b.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.png',
        'http://c.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.png',
        'http://d.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.png'
    ],
    grids: [
        'http://a.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.grid.json',
        'http://b.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.grid.json',
        'http://c.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.grid.json',
        'http://d.tiles.mapbox.com/devseed/1.0.0/devseed.about-devseed/{z}/{x}/{y}.grid.json'
    ],
    center: [-77, 48, 3],
    minzoom: 3,
    maxzoom: 5,
    formatter: "function(o,d) { return ({ teaser: function(o) { return o.title ? 'Click to view <strong>' + o.title + '</strong>.' : null; }, full: function() {}, location: function(o) { return o.url ? '' + o.url : null } })[o.format](d) }"
};

var mm = com.modestmaps;
var m = new mm.Map('poster',
    new wax.mm.connector(tilejson),
    null,
    [new mm.MouseHandler(), new mm.TouchHandler()]);
m.setCenterZoom(new mm.Location(
    tilejson.center[1],
    tilejson.center[0]),
    tilejson.center[2]);
wax.mm.interaction(m, tilejson);

var loop = dso.loop(m, [
    // DC
    { location: new mm.Location(48, -77), zoom: 3 },
    // Kabul
    { location: new mm.Location(34.6, 52), zoom: 5 },
    // Horn of Africa
    { location: new mm.Location(0, 55.0), zoom: 5 },
    // Geneva
    { location: new mm.Location(46.1, -8), zoom:5 },
    // US School Districts
    { location: new mm.Location(30, -85), zoom:5 },
    // Congo DRC
    { location: new mm.Location(-4, 11), zoom:5 },
    // FATA Pakistan
    { location: new mm.Location(33, 86.5), zoom:5 },
    // Honshu
    { location: new mm.Location(35.8, 152), zoom: 5 },
    // NodeConf
    { location: new mm.Location(46, -134), zoom:5 },
    // Port Au Prince
    { location: new mm.Location(20, -60), zoom:5 }
]);
mm.addEvent(m.parent, 'mousedown', loop.cancel);
mm.addEvent(m.parent, 'touchstart', loop.cancel);
$('#player a.playpause').click(function() {
    if ($('#player').hasClass('paused')) {
        loop.start(true);
    } else {
        loop.cancel();
    }
    return false;
});
$('#player a.prev').click(loop.prev);
$('#player a.next').click(loop.next);
loop.start();
</script>




