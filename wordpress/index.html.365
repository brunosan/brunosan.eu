  function showVideo(pos){
    $("#LeftPlayBtn").addClass("hide"); 
    $("#RightPlayBtn").addClass("hide"); 
    $("#RghtContent").addClass("hide"); 
    $("#LeftContent").addClass("hide"); 
    $("#divLeftVideoDetail").addClass("hide");
    $("#divRightVideoDetail").addClass("hide");
    $("#div"+ pos +"VideoDetail").removeClass("hide");
    $("#"+pos+"PlayBtn").removeClass("hide"); 
    $("#"+pos+"Content").removeClass("hide"); 
  }


  function showExternalContent(id, w, h, l){
  $.ajax({ type:"GET", url: id +".htm",
  success:function(response){

    $("#divExternalContent").html(response); }
  });
   $("#divExternalContent").animate({width:w, height:h, left:l, top:0}, 564);
  }

  function showDetail(thumbNo){
    var x = $("#divIntro").offset().left;
    var y = $("#img" + thumbNo).offset().top;
    var n = ($("#img" + thumbNo).offset().left - x - 1) / 226;

    $(".pop").addClass("hide"); 
    $(".btnPlay").addClass("hide");
    $("#divPlayBtn"+ thumbNo).removeClass("hide");
    $("#thumb"+ thumbNo).css("top", "-32px");
    $("#episode"+ thumbNo).css("top", "-32px");
    $("#tTitle"+ thumbNo).css("top", "-32px");    
    
    if(!n) 
      $("#pop"+thumbNo).removeClass("hide");
    else if(n == 1) 
      $("#pop"+thumbNo).removeClass("hide");
    else if(n == 2)
      $("#pop"+thumbNo).removeClass("hide");
    else
      $("#pop"+thumbNo).removeClass("hide");
    
    if(n < 2)
      $("#pop"+thumbNo).addClass("popRight");
    else
      $("#pop"+thumbNo).addClass("popLeft");
    $("#divInfoUpDetail").css("left", (n < 2 ? 248 + n * 226 : 340 + (n - 3) * 226));
    $("#divInfoUpDetail").css("background-image", "url(image/"+ (n < 2 ? "leftCallOut.png" : "rightCallOut.png") +")");
    $("#divInfoLeft").removeClass("hide");
  }

