function validatemaillist()
{

	var email=document.forms['inputForm'].email.value;
	var country=document.forms['inputForm'].country.value;
	var msg='';
	var cnt=0;
	if(email=='')
	{
	msg+="E-mail should not be empty\n";
	
	cnt++;
	}
	if(country=='')
	{
	msg+="Country should not be empty\n";
	cnt++;
	}
	if(email!='' && validate(email)==false)
	{
	msg+="InValid Email Address\n";
	cnt++;
	}
	if(cnt>0)
	{
	alert(msg);
	return false;
	}
}
function validate(address) {
   var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
   //var address = document.forms[form_id].elements[email].value;
   if(reg.test(address) == false) {
      //alert('Invalid Email Address');
      return false;
   }
}
function cleartext(obj)
{
  if(obj.value=='Email Address')
  {
    obj.value='';
  }
}

