// elementos de Local colocados en columnas
var NUM_ELEMENTOS_COLUMNA_LOCAL = 9;
var nombreDivLocal = 'li_7_seccion_1173_es_ES';
var contadorLocal = 0;
var htmlLocal = '';

//Creamos los divs vacios que formaran las estructuras de las columnas
var $divs = '';
for (var i = 0; i<NUM_ELEMENTOS_COLUMNA_LOCAL; i++){
	$divs += '<div class="columna_menu"></div>'
}
$divs = $($divs);

//Calculamos el numero de elementos que debemos a�adir en cada columna
//Redondeamos siempre hacia arriba de manera que si tenemos 10.5 elementos en cada columna, en realidad meteremos 11 por columna para que
//estas columnas queden igualadas de izquierda a derecha
var elem_in_colum = Math.ceil($("#" + nombreDivLocal + " > ul.cerrado > li, #" + nombreDivLocal + " > ul.seleccionado > li").size() / NUM_ELEMENTOS_COLUMNA_LOCAL);

var col = 0; //Columna actual en la que insertaremos cada elemento
var elem = 1; // Guia que nos servira para saber cuando cambiar de columna
$("#" + nombreDivLocal + " > ul.cerrado > li, #" + nombreDivLocal + " > ul.seleccionado > li").each(function(index){
	//index % NUM_ELEMENTOS_COLUMNA_LOCAL;
	//Obtenemos el div en el que tenemos que a�adir el nuevo codigo
	var $div = $($divs.get(col));
	
	var $p = $('<p id="' + $(this).attr('id') + '" class="inactivo"></p>');
	$p.append($(this).children('a'));
	
	$div.append($p);
	//Comprobamos si debemos cambiar de columna
	if (elem < elem_in_colum){
		elem++;
	}else{
		elem = 1;
		col++;
	}
			
});
$("#" + nombreDivLocal + " > ul.cerrado, #" + nombreDivLocal + " > ul.seleccionado").empty().append($divs);
// ---------------------------------------------------------------------------
// ---------------------------------------------------------------------------


// elementos de Programas colocados en columnas
var NUM_ELEMENTOS_COLUMNA_PROGRAMAS = 7;
var nombreDivProgramas = 'li_7_seccion_1159_es_ES';
var contadorProgramas = 0;
var htmlProgramas = '';

$("#" + nombreDivProgramas + " > ul.cerrado > li, #" + nombreDivProgramas + " > ul.seleccionado > li").each(function(){
	var html = $(this).html();
	if(html.indexOf('<ul')>0) html = html.substring(0,html.indexOf('<ul'));

	var div = '';
	if(contadorProgramas % NUM_ELEMENTOS_COLUMNA_PROGRAMAS == 0){
		if(contadorProgramas>0) div += '</div>';
		div += '<div class="columna_menup">';
	}
	html = div + '<p id="' + $(this).attr('id') + '" class="inactivo">' + html + '</p>';
	
	htmlProgramas += html;
	contadorProgramas++;
});

htmlProgramas += '</div>';

$("#" + nombreDivProgramas + " > ul.cerrado, #" + nombreDivProgramas + " > ul.seleccionado").html(htmlProgramas);

});
