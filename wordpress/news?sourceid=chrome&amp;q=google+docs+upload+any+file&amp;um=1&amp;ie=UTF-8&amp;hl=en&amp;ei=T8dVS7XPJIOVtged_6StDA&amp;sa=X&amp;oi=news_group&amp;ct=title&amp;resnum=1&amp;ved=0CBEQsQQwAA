t[&quot;name&quot;] = &quot;hp&quot;;
grabJsBundle(&quot;http://www.gstatic.com/news/js/850709098-en-anchorman_main_jsmod.js&quot;);
loadImages();

} catch (e) {
NEWS_logError(e, &quot;Error on onload handler&quot;);
}">
