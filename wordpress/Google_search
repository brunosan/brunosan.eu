						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading"><span dir="auto">Google Search</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub">  (Redirected from <a href="/w/index.php?title=Google_search&amp;redirect=no" title="Google search">Google search</a>)</div>
				<!-- /subtitle -->
