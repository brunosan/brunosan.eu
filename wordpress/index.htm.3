function replaceSubstring( sn, s1, s2)
{
	var	retStr;
	var           i;
	var	ch;
	// init
	retStr = "";
	for( i=0; i<sn.length; i++)
	{
	     	ch = sn.substr(i, 1);
                     	if( ch == s1 )
			retStr = retStr + s2;
		else
			retStr = retStr + ch;
	}
	return(retStr);
}
</script>
