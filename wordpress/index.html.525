	var id = "recentImg";
	var myFlashContent = swfobject.createSWF(att, par, id);
    }
    
    $('#list1a').accordion({
	animated:'slide',
	event:"mouseover",
	header:'.aTitle'});
    
    var fact=generateFacts();
    
    $('<p />').attr('id','fact').append('<strong>Fact:</strong> '+fact).appendTo('#facts');
    
    $('#nextfact').click(function() {
	fact = generateFacts();
	$('#fact').html('');
	$('#fact').append('<strong>Fact:</strong> '+fact);
	return false
    });  
    
    var innerwidth = $(window).width() - 25;
    var open = 0;
    $(window).resize(function() {
        $('#pulloutmenu').css('left',$(window).width() - 25);
	$('#pulltab').css({'background-image':'url(/assets/img/site/pulltab0.png)'});
	open = 0;
    });
    
    $('#pulloutmenu').css({
        left : innerwidth
    });
    
    $('#pulltab').click(function(e) {
        switch(open) {
            case 0:
                $('#pulloutmenu').animate({
                    left : '-=200'                   
                }, function() {
                    open++;
                    $('#pulltab').css({'background-image':'url(/assets/img/site/pulltab1.png)'});
                });
                break;
            
            case 1:
                $('#pulloutmenu').animate({
                    left : '+=200'
                }, function() {
                    open--;
                    $('#pulltab').css({'background-image':'url(/assets/img/site/pulltab0.png)'});
                });
                break;
        }
    });
    $('#pulloutmenu').css('visibility','visible');
});
