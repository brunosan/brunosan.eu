<script>
    amznJQ.onReady('jQuery', function() {
        jQuery.ajax({
            url: "/gp/gc/widget/product-gc?ie=UTF8&productGCWeblabTreatment=T5&asin=0415399173&productGroupID=gl_book",
            type: 'GET',
            async: true,
            dataType: 'html',
            tryCount: 0,
            retryLimit: 3,
            success: function (data, status, request) {
                jQuery('#product-gc').html(data);
            },
            error: function (data, status, request) {
                   // simple retry mechanism;
                   if (this.tryCount++ < this.retryLimit) {
                       jQuery.ajax(this);
                   } else { }
                }
        });
    });
</script>


  









  







