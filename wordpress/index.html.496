				var id = $(item).attr("data-id");
				var data = getlinks(id);
				if (data) {
					$("#overlay").html(data);
					$("#overlay").slideDown("slow");					
					clearTimeout(t);
					t = setTimeout(function() {						
						$("#overlay").fadeOut('slow');						
					}, 5000);
				}	
				else $("#overlay").fadeOut('slow');						
			});
			
