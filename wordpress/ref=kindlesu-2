            updateShovelerOnChange("W6124_tab4", "W6124_4");
        jQuery(this).find(".cBoxInner").blur();
    }).mouseover(function() {
        jQuery(this).addClass("kindleTab-tab-hover");
    }).unbind('keypress').keypress(function(event) {
        if (event.keyCode == '13') // if it is the enter key
        {
            event.preventDefault();
            KTActivateTab('W6124_tab4', 'W6124_4', 'kindle-shoveler-6124');
                updateShovelerOnChange("W6124_tab4", "W6124_4");
            jQuery(this).find(".cBoxInner").blur();
        }
    }).mouseout(function() {
        jQuery(this).removeClass("kindleTab-tab-hover");
    });
</script>
