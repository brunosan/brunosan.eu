<script type='text/javascript'>
amznJQ.onReady('kindleDeviceJS', function() {
  BBPopover.sessionId = '000-0000000-0000000';
  var refTag = 'kin3w_ddp_pop4';
  var learnMoreLink = jQuery("#bb_lm_B004SD1XB0").attr("href");
  if ( learnMoreLink ) {
    var refTagPattern = /ref=\w*/;
    refTag = refTagPattern.exec( learnMoreLink );
    refTag = new String(refTag).substr( 4 ); 
  }

  BBPopover.refTag['B004SD1XB0'] = refTag;
  BBPopover.registerDynamicPopover( 'B004SD1XB0', function() {
    BBPopover.selectedAsin['B005PB2SZY'] = 'B004SD1XB0';
    BBPopover.displayedAsin['B005PB2SZY'] = 'B004SD1XB0';
    BBPopover.originalAsin['B005PB2SZY'] = 'B004SD1XB0';
    jQuery.ajax({
      cache: false,
      data: {
        "asin": "B004SD1XB0",
        "asinCSVList" : "",
        "sId" : BBPopover.sessionId,
        "baseRef" : BBPopover.refTag['B004SD1XB0'],
        "isFirstLoad" : 1,
        "bundlePriceOverride" : ""
      },
      dataType: "json",
      success: function(data, status) {
        if ( data && data.error == 0 ) {
          for ( var key in data ) {
            if ( key == "error" ) {
              continue;
            }
            BBPopover.asinCache[key] = data[key];
            jQuery('#bb_extra_B004SD1XB0').children('.popoverLoading').css('display','none');
            BBPopover.doUpdate( 'B005PB2SZY', key, 1 );
            /* Pull in the image size from its parent object setting */
            var imgParam = jQuery('#bb_extra_feature_B005PB2SZY_productImage').css('height');
            jQuery('#bb_extra_feature_B005PB2SZY_productImage img').css('height',imgParam);
            /* Display all product info after aloading*/
            jQuery('#bb_extra_B004SD1XB0').children('.popoverLoaded').css('display','block');
          }
        }
      },
      type: "GET",
      url: "/gp/digital/fiona/ajax/buyBoxTwister/ref=" + BBPopover.refTag['B004SD1XB0'] + "_bbpfl"
    });
  });
});
</script>






















  
  

























