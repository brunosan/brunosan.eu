    jQuery("#W6122_tab1").unbind('click').click(function(event) {
        event.preventDefault();
        KTActivateTab('W6122_tab1', 'W6122_1', 'kindle-shoveler-6122');
            updateShovelerOnChange("W6122_tab1", "W6122_1");
        jQuery(this).find(".cBoxInner").blur();
    }).mouseover(function() {
        jQuery(this).addClass("kindleTab-tab-hover");
    }).unbind('keypress').keypress(function(event) {
        if (event.keyCode == '13') // if it is the enter key
        {
            event.preventDefault();
            KTActivateTab('W6122_tab1', 'W6122_1', 'kindle-shoveler-6122');
                updateShovelerOnChange("W6122_tab1", "W6122_1");
            jQuery(this).find(".cBoxInner").blur();
        }
    }).mouseout(function() {
        jQuery(this).removeClass("kindleTab-tab-hover");
    });
</script>
