<script type="text/javascript">

  var numCellsW6122_1 = 20; 
  var asinsW6122_1 = ['B002YKOXB6','B002MQYOFW','B003I1WY2A','B000QCS8TW','B00338QENI','B0015DROBO','B0036S4CTS','B003BW0CB6','B003JTHXOK','B000RH0C8G','B0042XA37Q','B0017SWPXY','B002F081N6','B0036S4C6G','B003UBTX7C','B003JTHXRW','B003UYUP58','B000FCKC4C','B003F1WM2K','B000OT8GD0'];
  var getEndpointW6122_1 = function(cellStart, numCells) {
    var url = '/gp/digital/fiona/ajax/asin-info?sId=000-0000000-0000000&asinList=';
    var delim = '';
    for (var i = cellStart; i < cellStart + numCells && i < asinsW6122_1.length; i++) { 
      url += delim + asinsW6122_1[i];
      delim = ',';
    }
    url += '&firstRef=_1_' + (cellStart + 1);
    url += '&imageHeight=110';
    return url;
  }

  var getCellContentW6122_1 = function(data) {
    if (data == null) { return ''; }


    var html = "\n      \n    <div style=\"height:110px\">\n      <a href=\"%productUrl\">\n        %image\n      </a>\n    </div>\n    <div>\n      <div style=\"height: 2.5em; overflow: hidden\">\n        <a title=\"%title\" href=\"%productUrl\">%title</a>\n      </div>\n      <div style=\"padding-top: 3px\">\n        %review\n        \n        <span class=\"price\">%price</span>\n      </div>\n          <div class=\"noJS-hidden\" style=\"padding-top: 10px; visibility:hidden;\">\n        <a href=\"javascript:void(0)\" onclick=\"TSAjaxCartAdd('%asin', '000-0000000-0000000');\" class=\"AjaxCartAdd_%asin\">\n          <img src=\"http://g-ecx.images-amazon.com/images/G/01/kindle/turing/btn-add-to-cart-md-pri._V192549178_.gif\" width=\"100\" alt=\"Add to cart\" height=\"22\" border=\"0\" />\n        </a>\n        <div style=\"display: none\" class=\"AjaxCartProcessing_%asin\">\n          <img src=\"http://g-ecx.images-amazon.com/images/G/01/kindle/turing/btn-processing-md-st._V192549173_.gif\" width=\"100\" alt=\"Processing\" height=\"22\" border=\"0\" />\n        </div>\n        <div style=\"white-space: nowrap; display: none\" class=\"AjaxCartRemove_%asin\">\n          <b>In Cart</b> (<a onclick=\"TSAjaxCartRemove('%asin', '000-0000000-0000000')\" href=\"javascript:void(0)\">undo</a>)\n        </div>\n      </div>\n    </div>\n";
    
    var asin = '', title = '', price = '', review = '', image = '', productUrl = '';
    
    if (data.asin) { asin = data.asin; }
    html = html.replace(/%asin/g, asin);
    
    if (data.productUrl) { productUrl = data.productUrl; }
    html = html.replace(/%productUrl/g, productUrl);
    
    if (data.title) { title = data.title.replace(/"/g, '&quot;'); }                                     
    html = html.replace(/%title/g, title);
    
    if (data.price && data.price.formatted) { 
      price = data.price.formatted; 
    } 
    html = html.replace(/%price/g, price);

    if(price == ''){
    html = html.replace(/hide-if-no-price/g, ';visibility:hidden;');
    }
    html = html.replace(/noJS-hidden/g, '');

    if (data.review) { review = data.review; }
    html = html.replace(/%review/g, review);
    
    if (data.image && data.image.tag) { 
        image = data.image.tag; 
    }
    html = html.replace(/%image/g, image);
    
    return html;
  };

  var loadedW6122_1 = false;
amznJQ.onReady('jQuery', function(){
  amznJQ.onReady('amazonShoveler', function() {
    shovelers['W6122_1'] = jQuery("#W6122_1").shoveler(getEndpointW6122_1, numCellsW6122_1, {
        cellTransformer: getCellContentW6122_1,
        onPageChangeCompleteHandler: function() { 
              if (loadedW6122_1) {
                setTimeout('TSAjaxCartUpdateStatus();', 0);
              }
	  },
        horizPadding: 10
    });

        jQuery("#W6122_1").addClass('kindleTab-active-content');
  });

  jQuery(document).ready(function() {
      loadedW6122_1 = true;
  });
});

</script>
