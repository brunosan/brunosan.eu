var variationsAdjuster = function() {jQuery('#jsSwatches .swatchInnerBorder:has(.swatchInnerText)').each(function(i, ele) {
    jQuery(this).css({'width':'100px','height':'45px'});
});jQuery('#jsSwatches .swatchInnerText').each(function(i, ele) {
    jQuery(this).css({'text-align':'center','white-space':'normal','line-height':'12px'});
});jQuery("#jsSwatches .swatchInnerText").each(function(i, ele) {
    if(jQuery(this).html() === "Kindle with Special Offers"){jQuery(this).html("With Special Offers<br>$79");jQuery(this).css({'margin-top':'5px','width':'94px'});}if(jQuery(this).html() === "Kindle without Special Offers"){jQuery(this).html("Without Special Offers<br>$109");jQuery(this).css({'margin-top':'5px','width':'94px'});}});
};
if( typeof jQuery == 'undefined' ) {
    amznJQ.onReady('jQuery', variationsAdjuster);
} else {
    variationsAdjuster();
}
