<script type="text/javascript">
    jQuery("#W6124_tab1").unbind('click').click(function(event) {
        event.preventDefault();
        KTActivateTab('W6124_tab1', 'W6124_1', 'kindle-shoveler-6124');
            updateShovelerOnChange("W6124_tab1", "W6124_1");
        jQuery(this).find(".cBoxInner").blur();
    }).mouseover(function() {
        jQuery(this).addClass("kindleTab-tab-hover");
    }).unbind('keypress').keypress(function(event) {
        if (event.keyCode == '13') // if it is the enter key
        {
            event.preventDefault();
            KTActivateTab('W6124_tab1', 'W6124_1', 'kindle-shoveler-6124');
                updateShovelerOnChange("W6124_tab1", "W6124_1");
            jQuery(this).find(".cBoxInner").blur();
        }
    }).mouseout(function() {
        jQuery(this).removeClass("kindleTab-tab-hover");
    });
</script>
