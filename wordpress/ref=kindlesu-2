    var url = '/gp/digital/fiona/ajax/asin-info?sId=000-0000000-0000000&asinList=';
    var delim = '';
    for (var i = cellStart; i < cellStart + numCells && i < asinsW6124_4.length; i++) { 
      url += delim + asinsW6124_4[i];
      delim = ',';
    }
    url += '&firstRef=_4_' + (cellStart + 1);
    url += '&imageHeight=110';
    return url;
  }

  var getCellContentW6124_4 = function(data) {
    if (data == null) { return ''; }


    var html = "\n      \n    <div style=\"height:110px\">\n      <a href=\"%productUrl\">\n        %image\n      </a>\n    </div>\n    <div>\n      <div style=\"height: 2.5em; overflow: hidden\">\n        <a title=\"%title\" href=\"%productUrl\">%title</a>\n      </div>\n      <div style=\"padding-top: 3px\">\n        %review\n        \n        <span class=\"price\">%price</span>\n      </div>\n          <div class=\"noJS-hidden\" style=\"padding-top: 10px; hide-if-no-price\">\n        <a href=\"javascript:void(0)\" onclick=\"TSAjaxCartAdd('%asin', '000-0000000-0000000');\" class=\"AjaxCartAdd_%asin\">\n          <img src=\"http://g-ecx.images-amazon.com/images/G/01/kindle/turing/btn-add-to-cart-md-pri._V192549178_.gif\" width=\"100\" alt=\"Add to cart\" height=\"22\" border=\"0\" />\n        </a>\n        <div style=\"display: none\" class=\"AjaxCartProcessing_%asin\">\n          <img src=\"http://g-ecx.images-amazon.com/images/G/01/kindle/turing/btn-processing-md-st._V192549173_.gif\" width=\"100\" alt=\"Processing\" height=\"22\" border=\"0\" />\n        </div>\n        <div style=\"white-space: nowrap; display: none\" class=\"AjaxCartRemove_%asin\">\n          <b>In Cart</b> (<a onclick=\"TSAjaxCartRemove('%asin', '000-0000000-0000000')\" href=\"javascript:void(0)\">undo</a>)\n        </div>\n      </div>\n    </div>\n";
    
    var asin = '', title = '', price = '', review = '', image = '', productUrl = '';
    
    if (data.asin) { asin = data.asin; }
    html = html.replace(/%asin/g, asin);
    
    if (data.productUrl) { productUrl = data.productUrl; }
    html = html.replace(/%productUrl/g, productUrl);
    
    if (data.title) { title = data.title.replace(/"/g, '&quot;'); }                                     
    html = html.replace(/%title/g, title);
    
    if (data.price && data.price.formatted) { 
      price = data.price.formatted; 
    } 
    html = html.replace(/%price/g, price);

    if(price == ''){
    html = html.replace(/hide-if-no-price/g, ';visibility:hidden;');
    }
    html = html.replace(/noJS-hidden/g, '');

    if (data.review) { review = data.review; }
    html = html.replace(/%review/g, review);
    
    if (data.image && data.image.tag) { 
        image = data.image.tag; 
    }
    html = html.replace(/%image/g, image);
    
    return html;
  };

  var loadedW6124_4 = false;
amznJQ.onReady('jQuery', function(){
  amznJQ.onReady('amazonShoveler', function() {
    shovelers['W6124_4'] = jQuery("#W6124_4").shoveler(getEndpointW6124_4, numCellsW6124_4, {
        cellTransformer: getCellContentW6124_4,
        onPageChangeCompleteHandler: function() { 
              if (loadedW6124_4) {
                setTimeout('TSAjaxCartUpdateStatus();', 0);
              }
	  },
        horizPadding: 10
    });

        jQuery("#W6124_4").addClass("kindleTab-inactive");
  });

  jQuery(document).ready(function() {
      loadedW6124_4 = true;
  });
});

</script>
