---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title type="text" xml:lang="en">{{ site.root_desc }}</title>
	<link type="application/atom+xml" href="http://brunosan.eu/feed/" rel="self"/>
 	<link type="text" href="http://brunosan.eu/" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://brunosan.eu/</id>
	<author>
		<name>Jason Heppler</name>
	</author>
	<rights>Copyright (c) 2004-2012, Bruno Sánchez-Andrade Nuño ; some rights reserved.</rights>

	{% for post in site.posts limit:10 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://brunosan.eu/{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://brunosan.eu/{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
