<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title type="text" xml:lang="en">{{ site.root_desc }}</title>
	<link type="application/atom+xml" href="/feed/" rel="self"/>
 	<link type="text" href="/" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>/</id>
	<author>
    <name> Bruno S치nchez-Andrade Nu침o </name>
	</author>
	<rights>Copyright (c) 2004-2012, Bruno S치nchez-Andrade Nu침o ; some rights reserved.</rights>

	{% for post in site.categories.blog limit:10 %}
	<entry>
		<title>{{ post.title }}</title>
    <link href="{{site.baseurl}}{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>/{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
