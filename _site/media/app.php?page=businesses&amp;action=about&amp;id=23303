<script type="text/javascript">
	function getGAPage(path, query) {
		if (typeof path == 'undefined') { path = document.location.pathname; }
		if (typeof query == 'undefined') { query = document.location.search; }

		// strip off all query string parameters except a couple exceptions.
		//
		if (! query.match(/(team_id|queryString|_te|utm_source|utm_medium|utm_campaign|utm_content)\=/)) {
			query = "";
		}

		return path + query;
	}

	var _gaq = [];
	_gaq.push(['_setAccount','UA-175897-4']);

	

	

	var ga_page = getGAPage();
	var trackPageView = true;

	

	// Track the page view
	//
	if (trackPageView) {

		// Track performance.
		// http://www.google.com/support/analyticshelp/bin/answer.py?hl=en&answer=1205784&topic=1120718
		//
		_gaq.push(['_trackPageview', ga_page],['_trackPageLoadTime']);

		   		 
		_gaq.push(function() {
			var elapsed = (new Date()).getTime() - kv.__start;
			$.ajax({
				url: kv.getAjaxUrl('logClientStats'),
				type: "POST",
				data: {elapsed : elapsed
						// Bundle page views tracking with client stats -- could split this part out
						// and put it up where GA is fired but then we'd be doing 3 requests per 1 page
						// Do this client-side to get more accurate, non-bot numbers.
						, page : 'businesses', action : 'loanView'}
			});
		});
		
	}

	

	// http://code.google.com/apis/analytics/docs/tracking/asyncTracking.html
	//
	(function(d) {
		var id='google-ga';
		var ga = d.createElement('script'); ga.id = id;ga.async = 1;
		var googleHost = 'www';
		if (document.location.protocol == 'https:') {
			googleHost = 'ssl';
		}
		ga.src = '//'+googleHost+'.google-analytics.com/ga.js';
		d.getElementById('ga-root').appendChild(ga);
	})(document);
