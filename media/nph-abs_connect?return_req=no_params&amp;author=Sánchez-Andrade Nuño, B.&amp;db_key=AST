<script type="text/javascript" src="http://code.jquery.com/jquery-1.5.1.min.js">&nbsp;</script>
<script type="text/javascript">
function submit_sel_bibs_to_service(service_name)
{
   // variable that will contain the bibcodes string
   var bibcodes = '';
   //get all the input checked
   var $inputs = $("input[name=bibcode]");
   $inputs.each(function() {
      if ($(this).attr('checked'))
         bibcodes = bibcodes + (($(this).val()).replace('&amp;', '&')) + ', ';
   });
   if ((bibcodes != '') && (service_name == 'authoraffiliations'))
   {
      $('#authoraffiliations_bibcodes').val(bibcodes);
      $('#authoraffiliations_form').submit();
   }
   else if ((bibcodes != '') && (service_name == 'adslabs_streamlined'))
   {
      $('#adslabs_streamlined_bibcodes').val(bibcodes);
      $('#adslabs_streamlined_form').submit();
   }
   else if ((bibcodes != '') && (service_name == 'ads_metrics'))
   {
      $('#ads_metrics_bibcodes').val(bibcodes);
      $('#ads_metrics_form').submit();
   }
   else if ((bibcodes != '') && (service_name == 'citehelper'))
   {
      $('#citehelper_bibcodes').val(bibcodes);
      $('#citehelper_form').submit();
   }
}
</script>
<form method="post" action="http://labs.adsabs.harvard.edu/ui/bibcodesearch/" id="adslabs_streamlined_form">
   <input type="hidden" name="bibcode" id="adslabs_streamlined_bibcodes" value="">
   <input type="button" value=" Export selected articles to ADSLabs " onclick="submit_sel_bibs_to_service('adslabs_streamlined');">
</form>
<br>
<form method="post" action="http://adsabs.harvard.edu/tools/metrics/" id="ads_metrics_form">
   <input type="hidden" name="bibcode" id="ads_metrics_bibcodes" value="">
   <input type="button" value=" Get Metrics Summary for selected articles " onclick="submit_sel_bibs_to_service('ads_metrics');">
</form>
<br>
 <form method="post" action="http://adsabs.harvard.edu/tools/authoraffiliations/" id="authoraffiliations_form">
   <input type="hidden" name="bibcodes" id="authoraffiliations_bibcodes" value="">
   <input type="button" value=" Get Author-Affiliation form for selected articles " onclick="submit_sel_bibs_to_service('authoraffiliations');">
</form>
<br>
 <form method="post" action="http://adsabs.harvard.edu/tools/citehelper/" id="citehelper_form">
   <input type="hidden" name="bibcode" id="citehelper_bibcodes" value="">
   <input type="button" value=" Run citation helper on selected articles " onclick="submit_sel_bibs_to_service('citehelper');">
</form>
