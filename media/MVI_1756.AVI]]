function drawInitialView()
{ 
// MSIE <10 redirect to root
if ($.browser.msie && parseInt($.browser.version) <10)
{
  if (currentPath != "/")
  {  
    //document.location.href='/?site#' + currentPath;
  //  post_to_url("/", { ie: true });
    document.body.innerHTML += '<form id="dynForm" action="/#' + currentPath.replace(/ /g,"+") +'" method="post"><input type="hidden" name="site" value="1"></form>';
document.getElementById("dynForm").submit();
	

  } else {
    if (document.location.hash.length>1 && !(document.location.hash.indexOf("_=_")>0))
    {
      currentHash = document.location.hash.replace(/\+/g," ").replace("#","");
      currentPath= window.location.hash.replace(/\+/g," ").replace("#","");  
	  var tmp = currentPath.split("?");
	  if (typeof tmp[1] != "undefined")
	  {
	    currentPath = tmp[0];
	  }
      log("url from hash");
    }
    
  }
} else {
    if (document.location.hash.length>1 && !(document.location.hash.indexOf("_=_")>0))
    {
			
      currentHash = document.location.hash.replace(/\+/g," ").replace("#","");
      currentPath= window.location.hash.replace(/\+/g," ").replace("#","");
     
	document.location.href=currentPath;
    } 
}


 	// jos blog tai home disabled niin redirect
 	if (currentPath == "/" && ($('#Photosite').hasClass('homedisabled') || !$('#Photosite').hasClass('homeactivated')))
 	{
 	 	if ($('#Photosite').hasClass('blogactivated') && !$('#Photosite').hasClass('homedisabled') )
	 	{
		 	currentPath="/blog/";
		 	
	 	} else{
		 	
		 	currentPath="/kuvat/";
	 	}
 	}   


  initialView=false;       



   if (pwlogged && !admin_mode)   showLoggedPw();

      
      
  
   if ($.browser.webkit) { $('body').addClass('Webkit'); }

   if( window.devicePixelRatio && window.devicePixelRatio >= 2 )
   {
	 $('body').addClass('retina');
   	}
   	// estettään ajax stop
   $(document).keypress(function(e) {
   if(e.keyCode == 27) {
     e.preventDefault();
   }
   });
   $(document).keydown(function(e) {
   if(e.keyCode == 27) {
     e.preventDefault();
   }
   });
   $(document).keyup(function(e) {
   if(e.keyCode == 27) {
     e.preventDefault();
   }
   });
   
/*

	$('.friendlist').droppable(
	{
	  accept: '#scroller li',
	  drop: function(x,y) {
	  	 if ($(y.draggable).attr('id').substring(0,8) == "friendid")	
 	    {
 	    	
 	    	$(y.draggable).appendTo("#" + $(this).attr('id'));
			
		} else{ 
	  	 alert($(y.draggable).attr('id') + " dropped over " + $(this).attr('id')); 
	  	 }
	  }
	});

	checkBrowser(); 
*/

	if (admin_mode)	updateFriends();


    if (config['settings']['community_share'] == 2 || (config['settings']['community_share'] == 1 && adminTools)) showSharingTool();
	positionGalleryFooter();
	updateOrientation();
    activateFSListener();
	
  	if (currentCart['numitems']) updateCartStatus();
  	
    $('#siteheader').disableSelection();
    
    if (admin_mode && adminTools) { 
    	$('body').removeClass('Visitorview');
    	$('body').addClass('Adminview');

		sidebarVisible=false;
   	}
 	$('#togglesitesearch').bind('click', function (e) {
 	  	e.stopPropagation(); 
 	  	$('#favoritesmenu').hide();$('#togglefavourites').removeClass('active');
 	  	$('#usermenu').hide(); $('#toggleusermenu').removeClass('active');
  		$('#ownsitesid').hide();   		$('#userdata').removeClass('active');
 	    if ($('#togglesitesearch').hasClass('active'))
    	{
    		
    		$('#sitesearch').unbind('click');
    		$('#sitesearch').hide();
    		$('#togglesitesearch').removeClass('active');
   		$('#ownsitesid').hide();   		$('#userdata').removeClass('active');
		

     	} else {
     	
			
    		
    		
    		
    		$('#Photosite').bind('click', function (e) {
    			if ($(e.target).parents('#sitesearch').length == 0)
    			{
      			  $('#Photosite').unbind('click');
	    		  $('#sitesearch').hide();
  	 	 		  $('#togglesitesearch').removeClass('active');
			    }
       		});
     		
     		
     		
     	     $('#sitesearch').bind('click', function ()
			 {
     		  	
     		  	e.stopPropagation();
     		  });
     	
     		
     		$('#sitesearch').css('width', $('#siteheadertools').width()+  "px");
     		
     		
    		$('#sitesearch').show();
    		$('#sitesearchbar').focus();
    		$('#togglesitesearch').addClass('active');
     	
     	}	
    
	
 	
 	});

    $('#toggleusermenu').bind('click', function (e) {
    	e.stopPropagation(); 
 //  	  	$('#usermenu').hide(); $('#toggleusermenu').removeClass('active');
   	  	$('#favoritesmenu').hide();$('#togglefavourites').removeClass('active');
    		$('#sitesearch').hide();   		$('#togglesitesearch').removeClass('active');
 		$('#ownsitesid').hide();   		$('#userdata').removeClass('active');

    	if ($('#toggleusermenu').hasClass('active'))
    	{
       		
    		$('#usermenu').hide();
    		$('#toggleusermenu').removeClass('active');
     	} else {
     	    $('#sitesearch').unbind('click');
     		  
     		$('#Photosite').bind('click', function () {
     			$('#Photosite').unbind('click');
	     		$('#usermenu').hide();
  	 	 		$('#toggleusermenu').removeClass('active');
       		});
     		
     		
     		
    		$('#usermenu').show();
    		$('#toggleusermenu').addClass('active');
     	
     	}	
    
    });
    
        
    $('#togglefavourites').bind('click', function (e) {
    	e.stopPropagation(); 
       		$('#sitesearch').hide();   		$('#togglesitesearch').removeClass('active');
  		$('#ownsitesid').hide();   		$('#userdata').removeClass('active');
	  	$('#usermenu').hide(); $('#toggleusermenu').removeClass('active');
 	  	if ($('#togglefavourites').hasClass('active'))
    	{
       		
    		$('#favoritesmenu').hide();
    		$('#togglefavourites').removeClass('active');
     	} else {
     	    $('#favoritesmenu').unbind('click');
    		$('#favoritesmenu').hide();
    		$('#togglefavourites').removeClass('active');
     		  
     		$('#Photosite').bind('click', function () {
     			$('#Photosite').unbind('click');
	     		$('#favoritesmenu').hide();
  	 	 		$('#togglefavourites').removeClass('active');
       		});
     		
     		var tmp = ($(window).width() - $('#togglefavourites').offset().left);
     		if (tmp<200) tmp =200;
     		$('#favoritesmenu').css('width',tmp + "px");
     		
    		$('#favoritesmenu').show();
    		$('#togglefavourites').addClass('active');
     	
     	}	
    
    });
    $('#sitesearchbar').bind('keyup',function (e) { if (e.keyCode ==13) { openSearch($('#sitesearchbar').val(),true); } });
    
    $('.searchsubmit').bind('click', function () {
    
    	openSearch($('#sitesearchbar').val(),true);
    
    });
   
   
   
   	if (logged_in) setTimeout("pullFeed();",1000);
    updateFeedBadge(badge);


	

	// DLT-IE7 FIX	
		
/*	var w=$('#servicelogin').width() + $("#dashboard").width() + $('#sitesettings').width() + $('#cartmanager').width() + $('#togglesitesearch').width() + $('#togglefavourites').width() + $('#userdata').width() + 
$('#toggleusermenu').width()+50;

	$('#siteheadertools').css('width',w + "px");

  */
  			
	navigateTo();
					
      

   
  

   var browserwarn=false;
   if ($.browser.msie && parseInt($.browser.version) <9) browserwarn=true;
   if ($.browser.webkit && parseInt($.browser.version) <4) browserwarn=true;
   
  
    if (loginwarn)
    {
    	loginwarn=false;
    	//if ($('#currentfolderid:visible').length>0)		


    	if (admin_mode && expirywarn && !lite)
    	{
			$('#notifications').show();
			openOverlay('notifications');
		} else if (admin_mode && lite) {
			$('#notifications').show();
			openOverlay('notifications');
		} else if (browserwarn) {
			openOverlay('browserwarning');
		
		} else if (logged_in && !admin_mode) {
						
	
			  $(function () {  openOverlay('loginwarn',false,false,false,'transparent'); });
	    }
		
    }

	if (!admin_mode && !(typeof folderdata[currentPath] == "undefined") && empty(folderdata[currentPath]['fc'])  && typeof subfolders[currentPath] == "undefined")
	{
		showloginhelper=true;		
  	}
  

												


}



