function prosideHideRelatedQ(){
    $("#proside-related-q").slideUp("fast",function(){
	$(this).next().addClass("first");
	$(this).parent("#relatedQueries").remove();
    }).find(".heading").removeClass("open");
}
function prosideLoadRelatedQ(callback){
    $("#proside-related-q .content").html('<div class="proside-loading"></div>');
    $("#proside-related-q .content").show();
    if (typeof jsonArray.relatedQueries != "undefined"){
        $.get("/input/queries.aside.jsp", {'id' : jsonArray.relatedQueries.id, 's' : jsonArray.relatedQueries.server, 'pro' : 1 }, function(data) {
	  $("#proside-related-q .content").delay(1000).slideUp("fast", function() {
	    var jqobj = $(data);
	    if (jqobj.get(0).id != "relatedq-wrap") {
	      prosideHideRelatedQ();
	    } else if (jqobj.find("a").length > 0 )
	      $(this).html(data).slideDown("fast").parent().find(".heading").addClass("open");
	    else prosideHideRelatedQ();
	  }).removeClass("empty");
	  if (typeof callback == "function")
	    callback();
	}).error(function(){
	  prosideHideRelatedQ();
	});
    } else {  prosideHideRelatedQ(); }
}
//prosideLoadRelatedQ();
