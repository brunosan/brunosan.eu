						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading"><span dir="auto">Cerebral palsy</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub"></div>
				<!-- /subtitle -->
																<!-- jumpto -->
				<div id="jump-to-nav" class="mw-jump">
					Jump to: <a href="#mw-head">navigation</a>,
					<a href="#p-search">search</a>
				</div>
				<!-- /jumpto -->
								<!-- bodycontent -->
				<div id="mw-content-text" lang="en" dir="ltr" class="mw-content-ltr"><table class="infobox" cellspacing="5" style="width:22em;">
<tr>
<th colspan="2" class="" style="text-align:center; font-size:125%; font-weight:bold; background-color: lightgrey">Cerebral palsy</th>
</tr>
<tr class="">
<td colspan="2" class="" style="text-align:center;"><i>Classification and external resources</i></td>
</tr>
<tr class="">
