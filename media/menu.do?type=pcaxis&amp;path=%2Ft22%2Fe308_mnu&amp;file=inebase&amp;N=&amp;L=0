<script type="text/javascript">
function mostrarMapa()
{
  document.getElementById("capaMapa").className='capagraficaVisible';
  document.getElementById("capaGrafico").className='capagraficaOculta';
  document.getElementById("capaTabla").className='capagraficaOculta';
}

function mostrarGrafico()
{
  document.getElementById("capaMapa").className='capagraficaOculta';
  document.getElementById("capaGrafico").className='capagraficaVisible';
  document.getElementById("capaTabla").className='capagraficaOculta';
}

function mostrarTabla()
{
  document.getElementById("capaMapa").className='capagraficaOculta';
  document.getElementById("capaGrafico").className='capagraficaOculta';
  document.getElementById("capaTabla").className='capagraficaVisible';
}


function keypres(e) {
  tecla_correcta=false;
  e = e || window.event;
  var code = e.keyCode || e.which;
  if(code == 13)
      tecla_correcta=true;
  return tecla_correcta;
}
</script>


